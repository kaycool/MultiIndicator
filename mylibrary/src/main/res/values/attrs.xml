<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="MultiIndicator">

        <attr name="multi_text_selected_color" format="color"/>
        <attr name="multi_icon_selected_color" format="color"/>
        <attr name="multi_text_selected_size" format="dimension"/>
        <attr name="multi_text_unselected_color" format="color"/>
        <attr name="multi_icon_unselected_color" format="color"/>
        <attr name="multi_text_unselected_size" format="dimension"/>
        <attr name="multi_indicator_color" format="color"/>
        <attr name="multi_indicator_height" format="dimension"/>
        <attr name="multi_indicator_width" format="dimension"/>
        <attr name="multi_indicator_equal_title" format="boolean"/>
        <attr name="multi_indicator_radius" format="dimension"/>
        <attr name="multi_indicator_style">
            <enum name="normal" value="0"/>
            <enum name="rectangle" value="1"/>
        </attr>
        <attr name="multi_max_height" format="dimension"/>
        <attr name="multi_max_lines" format="float"/>

    </declare-styleable>

    <declare-styleable name="MultiFlowLayout">
        <attr name="multi_max_selected_count" format="integer"/>
        <attr name="multi_max_selected_Tips" format="string"/>
        <attr name="multi_flow_padding_horizontal" format="string"/>
        <attr name="multi_flow_padding_vertical" format="string"/>
        <attr name="multi_flow_max_height" format="dimension"/>
        <attr name="multi_flow_max_lines" format="float"/>
    </declare-styleable>

</resources>